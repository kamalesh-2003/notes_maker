{
    "manifest_version": 3,
    "name": "Study Notes Summarizer",
    "version": "1.0",
    "permissions": ["scripting", "activeTab", "storage"],
    "host_permissions": ["http://127.0.0.1:5000/*"],
    "action": {
      "default_popup": "popup.html"
      
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  